// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model User{
    id String @id @default(uuid())
    email String
    name String
    password String
}
